
import java.util.Scanner;
public class InputExampleBasiccondition
{  
    public static void main (String[] args)
    {   
        // you can use any other meaningful 
        // variable name instead of "keyboard"
        Scanner keyboard; 
        keyboard = new Scanner ( System.in );
        // "System.in" is default input device for 
        // our computer system
        
        System.out.println("Please enter 2 integer number"); //prompt
        int x,y;
       x= keyboard.nextInt();
       y=keyboard.nextInt();
       if(x>y)
       {System.out.println("first");}
       else
       {System.out.println("porerta boro");
       System.out.println("porerta holo=  "+y);
       System.out.println("r prothom ta holo=  "+x);}
    }}