import java.util.Scanner;
public class P5{
  public static void main(String[] args){
    Scanner k=new Scanner(System.in);
    System.out.print("enter line limit");
    int input=k.nextInt();
    for(int i=1;i<=input;i++){ 
     for(int j=1;j<=i;j++)
      System.out.print("*");
     System.out.println();
    }
  }
}