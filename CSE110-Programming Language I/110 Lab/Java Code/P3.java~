import java.util.Scanner;
public class P3{
  public static void main(String[] args){
    Scanner k=new Scanner(System.in);
    System.out.print("enter line limit");
    int input=k.nextInt();
    for(int i=1;i<=input;i++){
      for(int j=1;j<=6;j++){
        System.out.print("*");}
      System.out.println();
  }
  }
}