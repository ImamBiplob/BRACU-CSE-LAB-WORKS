import java.util.Scanner;
public class HP2 {
  public static void main(String[] args) {
    Scanner k=new Scanner(System.in);
        int i,j,m,n;
    m=k.nextInt();
    n=k.nextInt();
    for(i=1;i<=m;i++){
      for(j=n;j>=1;j--){
      if(j<=i)
      System.out.print(j);
    else
      System.out.print(" ");
      }
      System.out.println();
    }
  }
}
