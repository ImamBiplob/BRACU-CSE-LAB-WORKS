public class ArrayShikkha1{
    public static void main (String[] args) {
        int[] a = new int[3];
        System.out.println("thikana,a="+a);
        System.out.println("kotogulo/capacity,a.length="+a.length);
        System.out.println("values,a[0]="+a[0]);
        System.out.println("values,a[1]="+a[1]);
        System.out.println("values,a[2]="+a[2]);
        
        
        
    }
}